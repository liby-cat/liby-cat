{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongo",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongo",
    "public": false
  },
  "ACL": {
    "dataSource": "mongo",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongo",
    "public": false
  },
  "org": {
    "dataSource": "mongo",
    "public": true
  },
  "catalog": {
    "dataSource": "mongo",
    "public": true
  },
  "user": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "usernameAvailable": true,
          "username2id": true,
          "create": true,
          "login": true,
          "logout": true
        }
      }
    }
  },
  "entry": {
    "dataSource": "mongo",
    "public": false
  }
}
