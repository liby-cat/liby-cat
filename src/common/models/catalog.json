{
  "name": "catalog",
  "plural": "catalog",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "catalogIndex": {
    "keys": {
      "orgIdx": 1,
      "catalogIdx": 1
    },
    "options": {
      "unique": true
    }
  },
  "properties": {
    "orgIdx": {
      "type": "string",
      "required": true
    },
    "catalogIdx": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true,
      "default": "books"
    },
    "creatorId":{
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "org": {
      "type": "belongsTo",
      "model": "org",
      "foreignKey": ""
    },
    "entries": {
      "type": "hasMany",
      "model": "entry",
      "foreignKey": ""
    },
    "owners": {
      "type": "referencesMany",
      "model": "user",
      "foreignKey": "ownerIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "readers": {
      "type": "referencesMany",
      "model": "user",
      "foreignKey": "readerIds",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {
    "owned": {
      "accepts": [
        {"arg": "options", "type": "object", "http": "optionsFromRequest"}
      ],
      "returns": {"type": "array", "root": true},
      "http": {"verb": "GET", "path": "/owned"}
    }
  }
}
